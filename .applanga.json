{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./package.json",
          "file_format": "chrome_i18n_json",
          "tag": ["app:messages.json", "another-tag"],
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./<language>.json",
          "file_format": "chrome_i18n_json",
          "tag": "app:messages.json",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}

